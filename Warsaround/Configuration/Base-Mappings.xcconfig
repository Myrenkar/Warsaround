PRODUCT_NAME = $(_PRODUCT_NAME)
PRODUCT_BUNDLE_IDENTIFIER = $(_BUNDLE_IDENTIFIER)
INFOPLIST_FILE = $(_INFOPLIST)

TARGETED_DEVICE_FAMILY = $(_DEVICE_FAMILY)
