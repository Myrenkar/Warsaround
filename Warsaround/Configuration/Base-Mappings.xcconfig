OTHER_LDFLAGS = $(_OTHER_LDFLAGS)

ASSETCATALOG_COMPILER_APPICON_NAME = $(_ASSET_ICON)
ASSETCATALOG_COMPILER_LAUNCHIMAGE_NAME = $(_ASSET_LAUNCHIMAGE)

